<struts>
	<package name="devTest1" namespace="/dev" extends="struts-default">
		
		<result-types>
			 <result-type name="json" class="org.apache.struts2.json.JSONResult" />
		</result-types>
		<action name="loadTest1Mgr">
			<result>dev-Test1-management.jsp</result>
		</action>
		<action name="saveOrUpdateTest1" class="Test1Action" method="saveOrUpdateTest1">
			<result name="input">
				dev-Test1-management.jsp
			</result>
			<result name="success" type="json">
				<param name="root">response</param>
			</result>
			<result name="error" type="json">
				<param name="root">response</param>
			</result>
		</action>
		<action name="loadTest1s" class="Test1Action" method="loadTest1s">
			<result name="success" type="json">
				<param name="root">response</param>
			</result>
			<result name="error" type="json">
				<param name="root">response</param>
			</result>
		</action>
	</package>
	
</struts>