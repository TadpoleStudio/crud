<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="devTest" namespace="/dev" extends="struts-default">
		
		<result-types>
			 <result-type name="json" class="org.apache.struts2.json.JSONResult" />
		</result-types>
		<action name="loadTestMgr">
			<result>dev-Test-management.jsp</result>
		</action>
		<action name="saveOrUpdateTest" class="TestAction" method="saveOrUpdateTest">
			<result name="input">
				dev-Test-management.jsp
			</result>
			<result name="success" type="json">
				<param name="root">response</param>
			</result>
			<result name="error" type="json">
				<param name="root">response</param>
			</result>
		</action>
		<action name="loadTests" class="TestAction" method="loadTests">
			<result name="success" type="json">
				<param name="root">response</param>
			</result>
			<result name="error" type="json">
				<param name="root">response</param>
			</result>
		</action>
	</package>
	
</struts>